{
  "name": "tmai-api",
  "version": "1.0.9",
  "description": "Token Metrics AI API JavaScript SDK - providing professional investors and traders with comprehensive cryptocurrency analysis, AI-powered trading signals, market data, and advanced insights.",
  "main": "dist/index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "jest",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "docs": "typedoc --out docs src/index.ts",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "token-metrics",
    "cryptocurrency",
    "trading",
    "ai",
    "api",
    "sdk"
  ],
  "author": "Token Metrics",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "rimraf": "^5.0.5",
    "supertest": "^7.1.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.4.2"
  }
}
